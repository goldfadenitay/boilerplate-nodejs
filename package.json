{
	"name": "boilerplate-nodejs",
	"version": "0.0.1",
	"description": "The purpose of this template is purely for experimenting and playing around",
	"scripts": {
		"prepare": "husky install",
		"container:dev": "cd backend/container && npm run dev",
		"container:build": "cd backend/container && npm run build",
		"container:start": "cd backend/container && npm run start",
		"container:docker:dev": "docker-compose -f docker-compose.dev.yml up --watch",
		"container:docker:prod": "docker-compose -f docker-compose.prod.yml up",
		"container:docker:build": "docker-compose build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/goldfadenitay/boilerplate-nodejs.git"
	},
	"keywords": [],
	"author": "",
	"devDependencies": {
		"husky": "^8.0.0",
		"prettier": "^3.2.0"
	}
}
